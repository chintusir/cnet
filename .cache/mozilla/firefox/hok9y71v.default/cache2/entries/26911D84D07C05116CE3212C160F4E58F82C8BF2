function roundHalf(num){for(var num=Math.round(2*num)/2,html="",i=num;i>=0;i--)i>=1?html+="<i class='fa fa-star'></i>":i>0&&(html+="<i class='fa fa-star-half-empty'></i>");return html}/*
 * Swipe 2.0
